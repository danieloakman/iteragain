{
  "name": "iteragain",
  "version": "3.18.1",
  "description": "Javascript Iterable/Iterator/Generator-function utilities.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:es": "tsc -p tsconfig.es.json",
    "build:clean": "ts-node ./scripts/build-clean.ts",
    "build:watch": "pnpm run build:clean && tsc --watch",
    "asyncify": "ts-node ./scripts/asyncify.ts",
    "asyncify:watch": "ts-node ./scripts/asyncify.ts -w",
    "asyncify:clean": "ts-node ./scripts/asyncify.ts -c",
    "lint": "eslint --ignore-path .eslintignore ./src --ext .ts",
    "format": "prettier --write **.ts",
    "test": "ts-mocha test/**.test.ts --reporter spec",
    "test:debug": "pnpm run test --inspect",
    "check": "tsc -p tsconfig.test.json",
    "check:watch": "pnpm run check --watch",
    "coverage": "nyc --skip-full ts-mocha test/**.test.ts --reporter spec",
    "bm": "ts-node benchmark/index.bm.ts",
    "bm:prof": "node -r ts-node/register --prof benchmark/index.bm.ts",
    "new:iterator": "ts-node ./scripts/new-iterator.ts",
    "new:function": "ts-node ./scripts/new-function.ts",
    "new:test": "ts-node ./scripts/new-test.ts",
    "typedoc": "typedoc --plugin typedoc-plugin-remove-references --includeVersion --entryPointStrategy expand src/ && git add docs",
    "v:patch": "pnpm version patch -f -m \"Upgrade package version to v%s\"",
    "v:minor": "pnpm version minor -f -m \"Upgrade package version to v%s\"",
    "v:major": "pnpm version major -f -m \"Upgrade package version to v%s\"",
    "prepare": "husky install",
    "preversion": "pnpm run lint",
    "postversion": "git push --follow-tags",
    "publish:all": "ts-node ./scripts/publish-all.ts"
  },
  "devDependencies": {
    "@types/benchmark": "^2.1.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.5",
    "@types/prettier": "^2.7.3",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "benchmark": "^2.1.4",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.0",
    "iterare": "^1.2.1",
    "iterplus": "^3.0.2",
    "ix": "^5.0.0",
    "js-utils": "github:danieloakman/js-utils#node",
    "lint-staged": "^13.2.2",
    "mocha": "10.2.0",
    "more-node-fs": "^2.0.2",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "rxjs": "^7.8.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.7",
    "typedoc-plugin-remove-references": "0.0.6",
    "typedoc-plugin-rename-defaults": "^0.6.5",
    "typescript": "^5.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danieloakman/iteragain.git"
  },
  "author": "Daniel Oakman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danieloakman/iteragain/issues"
  },
  "homepage": "https://www.npmjs.com/package/iteragain",
  "keywords": [
    "iterator",
    "iterable",
    "IterableIterator",
    "itertools",
    "iteration",
    "iter",
    "generator",
    "iterutil",
    "iteragain",
    "es6",
    "es2015",
    "modules",
    "map",
    "filter",
    "reduce",
    "lazy",
    "array",
    "utility",
    "util"
  ],
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix"
    ]
  }
}
