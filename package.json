{
  "name": "iteragain",
  "version": "0.1.0",
  "description": "Javascript ES6 Iterable/Iterator utilities.",
  "main": "index.js",
  "types": "index.d.ts",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "build": "tsc --build",
    "build:clean": "ts-node ./scripts/build-clean.ts",
    "build:watch": "npm run build:clean && tsc --watch",
    "lint": "eslint --ignore-path .eslintignore ./src --ext .ts",
    "format": "prettier-eslint --ignore .gitignore --write \"./src/**/*\"",
    "test": "ts-mocha test/**.test.ts --reporter list",
    "bm": "ts-node benchmark/index.bm.ts",
    "v:patch": "npm version patch -m \"Upgrade package version to v%s\"",
    "v:minor": "npm version minor -m \"Upgrade package version to v%s\"",
    "v:major": "npm version major -m \"Upgrade package version to v%s\"",
    "prepare": "npm run build && husky install",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run lint",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/benchmark": "^2.1.1",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "benchmark": "^2.1.4",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.0",
    "iterare": "^1.2.1",
    "iterplus": "^3.0.2",
    "mocha": "^8.2.1",
    "more-node-fs": "^2.0.1",
    "prettier": "^2.6.2",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^6.0.1",
    "ts-mocha": "^9.0.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danieloakman/iteragain.git"
  },
  "author": "Daniel Oakman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danieloakman/iteragain/issues"
  },
  "homepage": "https://github.com/danieloakman/iteragain#readme",
  "keywords": [
    "iterator",
    "iterable",
    "itertools",
    "iteration",
    "iter",
    "generator",
    "iterutil",
    "iteragain",
    "es6",
    "es2015"
  ]
}
